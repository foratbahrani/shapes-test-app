{
	"env": {
		"browser": true,
		"es6": true,
		"commonjs": true,
		"node": true,
		"jest": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 9,
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"default-case": "error",
		"dot-notation": "error",
		"no-self-compare": "warn",
		"no-useless-concat": "error",
		"no-unused-vars": "warn",
		"spaced-comment": "warn",
		"prefer-const": "warn",
		"no-useless-constructor": "error",
		"react/prop-types": "off",
		"camelcase": [
			"warn",
			{
				"ignoreDestructuring": true,
				"ignoreImports": true
			}
		]
	}
}
